{
  "title": "Switch work spaces",
  "rules": [
    {
      "description": "switch to workspace left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_shift"
              ]
            },
            "key_code": "open_bracket"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "switch to workspace right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_shift"
              ]
            },
            "key_code": "close_bracket"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "right_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    }
  ]
}
